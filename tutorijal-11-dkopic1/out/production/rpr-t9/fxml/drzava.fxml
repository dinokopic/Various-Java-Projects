<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="150.0" prefWidth="270.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@/css/ispravnost.css" />
    </stylesheets>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="144.0" minWidth="10.0" prefWidth="86.0" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="211.0" minWidth="10.0" prefWidth="164.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label text="%nazivUnos" />
      <Label text="%glavniGrad" GridPane.rowIndex="1" />
      <TextField fx:id="fieldNaziv" GridPane.columnIndex="1" />
      <ChoiceBox fx:id="choiceGrad" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <ButtonBar nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0" prefWidth="250.0" GridPane.columnSpan="2" GridPane.rowIndex="2">
        <buttons>
          <Button fx:id="btnOk" mnemonicParsing="false" onAction="#onOk" text="Ok" />
            <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#onCancel" text="Cancel" />
        </buttons>
         <GridPane.margin>
            <Insets bottom="10.0" />
         </GridPane.margin>
      </ButtonBar>
   </children>
   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
</GridPane>
